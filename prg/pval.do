clear all*set color scheme for graphs	set scheme s1colorset obs 500gen lcl = 1.01gen ucl = 10.01gen lclb = 0.90gen uclb = 2.81gen alpha = 0.05******	gen lnrr = (log(ucl)+ log(lcl))/2	gen lnse = (log(ucl)- log(lcl))/(2*invnormal(1-alpha/2))	gen rr = exp(lnrr)		gen z = (_n-250)/50	gen rrci = rr*exp(z*lnse)	gen p = normal(-abs(z))*2	gen ci = (p-1)*100***	gen lnrrb = log(uclb*lclb)/2	gen lnseb = log(uclb/lclb)/(2*invnormal((1-alpha/2)))	gen rrb = exp(lnrrb)		gen zb = (_n-250)/50	gen rrcib = rrb*exp(z*lnseb)	gen pb = normal(-abs(zb))*2	gen cib = (pb-1)*100		label var p "RR(95% CI) = (1.01, 10.01)"	label var pb "RR(95% CI) = (0.90, 2.81)"		label define cif -20 "20" -40 "40" -60 "60" -80 "80" -100 "100"	label value ci cif	*label var ci "Confidence interval %"		summarize	twoway 	(line p rrci,  xlabel(0 1 2 10 15 30) xmtick(.2(.1)2 2(1)10 10(10)30) ylabel(,format(%2.1f) angle(0)) xscale(log range(0.2 40)) ytitle("P value") lcolor(black) yaxis(1)) ///			 (line pb rrcib,  xlabel(0 1 2 10 15 30) xmtick(.2(.1)2 2(1)10 10(10)30) xscale(log range(0.2 40)) ytitle("P value") xline(1,   ///			 					lcolor(black)) lcolor(blue) legend(rows(2) ring(0) pos(1) size(small) order(1 2 ) symxsize(*.5)  )) ///			 				(scatter ci rrci, yaxis(2) ylabel(,valuelabel axis(2) angle(0)) ytitle(Confidence interval %, axis(2) orientation(rvertical)) ///			 					msymbol(i) xscale(log range(0.2 40)) xtitle(Hypothetical Risk ratio))			 					